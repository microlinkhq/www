    "@storybook/addon-console": "latest",
    "@storybook/addon-viewport": "latest",
    "@storybook/addons": "latest",
    "@storybook/react": "latest",
    "ava": "latest",
    "contrast": "latest",
    "conventional-github-releaser": "latest",
    "debug-logfmt": "latest",
    "esm": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "nodengine": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "pretty-bytes": "latest",
    "puppeteer": "latest",
    "rgb-hex": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest",
    "urlint": "latest"
  },
  "engines": {
    "node": "12.x"
  },
  "files": [
    "data",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "src",
    "static"
  ],
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "build-storybook": "NODE_ENV=production build-storybook",
    "build:data": "DEBUG=data:providers* node scripts/fetch-data",
    "clean": "rm -rf node_modules && npm run clean:data && npm run clean:build",
    "clean:build": "gatsby clean",
    "clean:data": "rm -rf data/*.json",
    "dev": "GATSBY_HOT_LOADER=fast-refresh gatsby develop --host localhost --port 8000",
    "lint": "standard-markdown README.md && standard",
    "postinstall": "npm run build:data",
    "postrelease": "npm run release:tags && npm run release:github",
    "posttest": "urlint https://microlink.io/sitemap.xml",
    "predev": "npm run build:data",
    "prerelease": "npm run update:check",
    "pretest": "npm run lint",
    "preversion": "git-authors-cli && git add package.json",
    "release": "standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "storybook": "start-storybook -p 6006",
    "test": "ava",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "private": true,
  "license": "MIT",
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "require": [
      "esm"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "finepack"
    ],
    "*.js": [
      "prettier-standard"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
