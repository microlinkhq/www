  "files": [
    "data",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "src",
    "static"
  ],
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "build-storybook": "NODE_ENV=production build-storybook",
    "build:data": "DEBUG=data:providers* node scripts/fetch-data",
    "build:timestamps": "node scripts/git-timestamps.js",
    "clean": "npm run clean:build && npm run clean:data && rm -rf node_modules",
    "clean:build": "gatsby clean",
    "clean:data": "rm -rf data/*.json",
    "contributors": "(npx git-authors-cli && npx finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "dev": "gatsby develop --host 0.0.0.0 --port 8000",
    "lint": "standard",
    "postinstall": "npm run transpile",
    "postrelease": "npm run release:tags && npm run release:github",
    "prebuild": "npm run build:data",
    "predev": "npm run build:data",
    "prepare": "simple-git-hooks",
    "pretest": "npm run lint",
    "preversion": "git-authors-cli && git add package.json",
    "release": "standard-version -a",
    "release:github": "github-generate-release",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "storybook": "start-storybook -p 6006",
    "test": "vitest --no-watch",
    "transpile": "echo \"export { getApiUrl } from '@microlink/mql'\" | esbuild --bundle --target=ES2019 --format=esm --minify-whitespace --minify-identifiers > src/helpers/get-api-url.js",
    "update": "ncu -i"
  },
  "private": true,
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0
      ]
    }
  },
  "nano-staged": {
    "*.js": [
      "prettier-standard"
    ],
    "*.md": [
      "node scripts/blog-slugger.js",
      "node scripts/static-assets.js"
    ],
    "package.json": [
      "finepack"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "node scripts/git-timestamps.js && npx nano-staged"
  },
  "standard": {
    "ignore": [
      "src/helpers/get-api-url.js"
    ]
  }
}
