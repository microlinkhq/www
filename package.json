{
  "name": "microlink-www",
  "description": "microlink website",
  "homepage": "https://github.com/microlink/microlink-www#readme",
  "version": "2.1.0",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microlink/microlink-www.git"
  },
  "bugs": {
    "url": "git+https://github.com/microlink/microlink-www.git"
  },
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "autoprefixer": "9.1.1",
    "color": "3.0.0",
    "cssnano": "4.0.5",
    "dlv": "1.1.2",
    "download": "7.1.0",
    "gatsby": "1.9.277",
    "gatsby-link": "1.6.46",
    "gatsby-plugin-canonical-urls": "1.0.18",
    "gatsby-plugin-catch-links": "1.0.24",
    "gatsby-plugin-google-analytics": "1.0.31",
    "gatsby-plugin-netlify": "1.0.21",
    "gatsby-plugin-postcss-sass": "1.0.22",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-react-next": "1.0.11",
    "gatsby-plugin-remove-trailing-slashes": "1.0.9",
    "gatsby-plugin-sharp": "1.6.48",
    "gatsby-plugin-sitemap": "1.2.25",
    "gatsby-plugin-styled-components": "2.0.11",
    "gatsby-source-filesystem": "1.5.39",
    "gatsby-transformer-javascript-frontmatter": "1.0.10",
    "gatsby-transformer-remark": "1.7.44",
    "gatsby-transformer-sharp": "1.6.27",
    "gatsby-transformer-yaml": "1.5.18",
    "got": "9.2.0",
    "human-number": "~1.0.1",
    "keyv": "3.0.0",
    "keyv-file": "0.1.7",
    "lodash.chunk": "4.2.0",
    "markdown-in-js": "1.1.4",
    "normalize.css": "8.0.0",
    "p-all": "1.0.0",
    "palx": "1.0.3",
    "parse-domain": "2.1.7",
    "postcss-focus": "3.0.0",
    "react": "16.4.2",
    "react-codecopy": "2.1.3",
    "react-dom": "16.4.2",
    "react-extras": "0.7.1",
    "react-feather": "1.1.1",
    "react-helmet": "5.2.0",
    "react-iframe": "1.2.0",
    "react-json-view": "1.19.1",
    "react-live": "1.11.0",
    "react-microlink": "2",
    "react-stripe-elements": "2.0.1",
    "react-syntax-highlighter": "10.1.0",
    "react-timeago": "4.1.9",
    "rebass": "2.1.0",
    "scroll-into-view-if-needed": "~2.2.20",
    "slugo": "0.2.1",
    "styled-components": "3.4.2",
    "styled-is": "1.1.5",
    "styled-system": "3.0.2",
    "unfetch": "3.1.1"
  },
  "devDependencies": {
    "babel-eslint": "latest",
    "husky": "latest",
    "imageoptim-cli": "latest",
    "lint-staged": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "svgo": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "data",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "src",
    "static"
  ],
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "clean": "rm -rf node_modules && npm run clean:urls",
    "clean:urls": "rm -rf node_modules/.cache/microlink.msgpack",
    "dev": "gatsby develop --host localhost --port 8000",
    "format": "prettier-standard --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "lint": "standard-markdown README.md && standard src/*/**.js",
    "prebuild": "npm run clean:urls && node scripts/fetch-urls",
    "predev": "node scripts/fetch-urls",
    "test": "exit 0"
  },
  "private": true,
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "finepack",
      "git add"
    ],
    "*.{png,jpeg,jpg,gif}": [
      "imageoptim --imagealpha",
      "git add"
    ],
    "*.svg": [
      "svgo --pretty",
      "git-add"
    ],
    "*.js": [
      "prettier-standard",
      "git add"
    ]
  },
  "standard": {
    "globals": [
      "React",
      "graphql",
      "requestAnimationFrame"
    ],
    "parser": "babel-eslint"
  }
}
