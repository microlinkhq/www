{
  "name": "microlink-www",
  "description": "microlink website",
  "homepage": "https://github.com/microlink/microlink-www#readme",
  "version": "2.2.11",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microlink/microlink-www.git"
  },
  "bugs": {
    "url": "git+https://github.com/microlink/microlink-www.git"
  },
  "keywords": [
    "gatsby"
  ],
  "dependencies": {
    "@microlink/react": "~3",
    "@rebass/components": "~4.0.0-1",
    "autoprefixer": "~9.4.7",
    "color": "~3.1.0",
    "cssnano": "~4.1.8",
    "cssnano-preset-advanced": "~4.0.6",
    "dlv": "~1.1.2",
    "download": "~7.1.0",
    "gatsby": "~2.0.112",
    "gatsby-link": "~2.0.10",
    "gatsby-plugin-canonical-urls": "~2.0.10",
    "gatsby-plugin-catch-links": "~2.0.10",
    "gatsby-plugin-google-analytics": "~2.0.13",
    "gatsby-plugin-netlify": "~2.0.8",
    "gatsby-plugin-react-helmet": "~3.0.6",
    "gatsby-plugin-remove-trailing-slashes": "~2.0.7",
    "gatsby-plugin-sass": "~2.0.10",
    "gatsby-plugin-sitemap": "~2.0.5",
    "gatsby-plugin-styled-components": "~3.0.5",
    "gatsby-source-filesystem": "~2.0.20",
    "gatsby-transformer-javascript-frontmatter": "~2.0.8",
    "gatsby-transformer-remark": "~2.2.4",
    "gatsby-transformer-yaml": "~2.1.8",
    "got": "~9.6.0",
    "human-number": "~1.0.2",
    "json-future": "~2.1.3",
    "keyv": "~3.1.0",
    "keyv-file": "~0.1.11",
    "lodash.chunk": "~4.2.0",
    "markdown-in-js": "~1.1.4",
    "node-sass": "~4.11.0",
    "normalize.css": "~8.0.1",
    "notepack.io": "~2.2.0",
    "p-all": "~1.0.0",
    "palx": "~1.0.3",
    "parse-domain": "~2.1.7",
    "polished": "~2.3.3",
    "postcss-focus": "~4.0.0",
    "react": "16.8.1",
    "react-codecopy": "~2.1.4",
    "react-dom": "16.8.0",
    "react-extras": "~0.9.0",
    "react-feather": "~1.1.6",
    "react-helmet": "~5.2.0",
    "react-iframe": "~1.3.3",
    "react-json-view": "~1.19.1",
    "react-live": "~1.12.0",
    "react-stripe-elements": "~2.0.3",
    "react-syntax-highlighter": "~10.1.2",
    "react-timeago": "~4.3.0",
    "rebass": "~3.0.1",
    "scroll-into-view-if-needed": "~2.2.20",
    "slugo": "~0.2.3",
    "styled-components": "~4.1.3",
    "styled-is": "~1.1.5",
    "styled-system": "~3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "babel-eslint": "latest",
    "conventional-github-releaser": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "husky": "latest",
    "imageoptim-cli": "latest",
    "lint-staged": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest",
    "svgo": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "data",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "src",
    "static"
  ],
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "clean": "rm -rf node_modules && npm run clean:urls",
    "clean:urls": "rm -rf node_modules/.cache/microlink.msgpack",
    "dev": "gatsby develop --host localhost --port 8000",
    "format": "prettier-standard --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "lint": "standard-markdown README.md && standard src/*/**.js",
    "postrelease": "npm run release:tags && npm run release:github",
    "prebuild": "npm run clean:urls && node scripts/fetch-demo-links",
    "predev": "node scripts/fetch-demo-links",
    "prerelease": "npm run update:check && git-authors-cli",
    "release": "git add package.json && standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "exit 0",
    "update": "ncu -a",
    "update:check": "ncu -- --error-level 2"
  },
  "private": true,
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "package.json": [
        "finepack",
        "git add"
      ],
      "*.{png,jpeg,jpg,gif}": [
        "imageoptim --imagealpha",
        "git add"
      ],
      "*.svg": [
        "svgo --pretty",
        "git-add"
      ],
      "*.js": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "standard": {
    "globals": [
      "React",
      "graphql",
      "requestAnimationFrame"
    ],
    "parser": "babel-eslint"
  },
  "standard-version": {
    "scripts": {
      "prechangelog": "git-authors-cli"
    }
  }
}
