  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@kikobeats/jsonsort": "latest",
    "@storybook/addon-a11y": "latest",
    "@storybook/addon-actions": "latest",
    "@storybook/addon-console": "latest",
    "@storybook/addon-viewport": "latest",
    "@storybook/addons": "latest",
    "@storybook/react": "latest",
    "ava": "latest",
    "conventional-github-releaser": "latest",
    "esm": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "husky": "latest",
    "imageoptim-cli": "latest",
    "lint-staged": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest",
    "svgo": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "data",
    "gatsby-browser.js",
    "gatsby-config.js",
    "gatsby-node.js",
    "src",
    "static"
  ],
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "build-storybook": "npm run prebuild && NODE_ENV=production build-storybook",
    "build:assets": "npx autotrack -o static/js/autotrack.min.js -p eventTracker",
    "clean": "rm -rf node_modules && npm run clean:urls && npm run clean:build",
    "clean:build": "gatsby clean",
    "clean:urls": "rm -rf data/demo-links.json",
    "dev": "gatsby develop --host localhost --port 8000",
    "fetch:urls": "node scripts/fetch-demo-links",
    "lint": "standard-markdown README.md && standard src/*/**.js",
    "postrelease": "npm run release:tags && npm run release:github",
    "prebuild": "npm run clean:urls && npm run clean:build && npm run fetch:urls && npm run build:assets",
    "predev": "node scripts/fetch-demo-links",
    "prerelease": "npm run update:check && git-authors-cli",
    "release": "git-authors-cli && git add package.json && standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "storybook": "start-storybook -p 6006",
    "test": "ava",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "private": true,
  "license": "MIT",
  "ava": {
    "require": [
      "esm"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "package.json": [
        "finepack",
        "git add"
      ],
      "*.{png,jpeg,jpg,gif}": [
        "imageoptim --imagealpha",
        "git add"
      ],
      "*.svg": [
        "svgo --pretty",
        "git-add"
      ],
      "*.js": [
        "prettier-standard",
        "git add"
      ],
      "scripts/demo-links.json": [
        "jsonsort --criteria logo --save --quiet",
        "git add"
      ]
    }
  },
  "standard": {
    "globals": [
      "React",
      "graphql",
      "requestAnimationFrame"
    ],
    "parser": "babel-eslint"
  }
}
